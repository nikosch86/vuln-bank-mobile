# Security Level 00: No Pinning + Trust User Certificates
# Same as L0 but also trusts user-installed CA certificates,
# making it easy to intercept with tools like Burp Suite.

SECURITY_LEVEL=l00
TRUST_USER_CERTS=true
ENABLE_SSL_PINNING=false
ENABLE_PROXY_DETECTION=false
USE_CUSTOM_SSL_MODULE=false
ENABLE_ANTI_TAMPERING=false
ENABLE_ROOT_DETECTION=false
